<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/main_navigation_graph"
            app:startDestination="@id/fileListFragment">
    <fragment
        android:id="@+id/fileListFragment"
        android:name="com.ayteneve93.apexexplorer.view.main.fragments.filelist.FileListFragment"
        android:label="FileListFragment"
        tools:layout="@layout/fragment_file_list">
        <action
            android:id="@+id/action_fileListFragment_to_favoriteFragment"
            app:destination="@id/favoriteFragment"/>
    </fragment>
    <fragment
            android:id="@+id/favoriteFragment"
            android:name="com.ayteneve93.apexexplorer.view.main.fragments.favorite.FavoriteFragment"
            android:label="FavoriteFragment"
            tools:layout="@layout/fragment_favorite">
    </fragment>
</navigation>